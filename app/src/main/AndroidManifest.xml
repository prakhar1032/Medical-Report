<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medical.medicalreport">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MedicalReport">
        <activity android:name=".DoctorVisit"></activity>
        <activity android:name=".Diseases" />
        <activity
            android:name=".MedicalReport"
            android:label="@string/title_activity_medical_report"
            android:theme="@style/Theme.MedicalReport.NoActionBar" />
        <activity android:name=".Medication" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>